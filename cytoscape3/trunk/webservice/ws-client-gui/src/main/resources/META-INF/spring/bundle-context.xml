<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:context="http://www.springframework.org/schema/context"
	xmlns:lang="http://www.springframework.org/schema/lang" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xsi:schemaLocation="
    http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
    http://www.springframework.org/schema/aop
    http://www.springframework.org/schema/aop/spring-aop-2.5.xsd
    http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-2.5.xsd
           http://www.springframework.org/schema/lang http://www.springframework.org/schema/lang/spring-lang-2.5.xsd
            http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi-1.0.xsd"
	default-lazy-init="false">

	<context:annotation-config />

	<bean id="unifiedNetworkImportDialog"
		class="org.cytoscape.webservice.client.gui.UnifiedNetworkImportDialog">
		<constructor-arg ref="cyDesktopServiceRef" />
		<constructor-arg value="false" />
		<constructor-arg ref="aboutDialogFactoryServiceRef" />
		<constructor-arg ref="webServiceClientManagerServiceRef" />
		<property name="cyNetworkManager" ref="cyNetworkManagerServiceRef" />
	</bean>

	<bean id="webServiceNetworkImportAction" 
	      class="org.cytoscape.webservice.client.gui.WebServiceNetworkImportAction">
		<constructor-arg ref="cyNetworkManagerServiceRef" /> 
		<constructor-arg ref="unifiedNetworkImportDialog" /> 
	</bean> 
<!--
	<bean id="webServiceContextMenuListener" 
	      class="org.cytoscape.webservice.client.gui.WebServiceContextMenuListener"/>
		 -->
</beans>
