<?xml version="1.0"?><project>
  <parent>
    <artifactId>cytoscape</artifactId>
    <groupId>cytoscape</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.cytoscape</groupId>
  <artifactId>view</artifactId>
  <packaging>bundle</packaging>
  <name>view</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <dependencies>

    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>org.cytoscape</groupId>
      <artifactId>network</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>geom.rtree</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>geom.spacial</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>util.intr</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>render.immed</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>render.stateful</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>render.export</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>phoebe</groupId>
      <artifactId>phoebe.dnd</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>undo</groupId>
      <artifactId>undo</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

	</dependencies>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.felix</groupId>
				<artifactId>maven-bundle-plugin</artifactId>
				<extensions>true</extensions>
				<configuration>
					<instructions>
						<Export-Package>org.cytoscape.view</Export-Package>
						<Private-Package>org.cytoscape.view.impl</Private-Package>
					</instructions>
				</configuration>
			</plugin>
		</plugins>
  </build>

</project>
