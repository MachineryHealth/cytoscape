<?xml version="1.0"?><project>
  <parent>
    <artifactId>cytoscape</artifactId>
    <groupId>cytoscape</groupId>
    <version>1.0-SNAPSHOT</version>
  </parent>
  <modelVersion>4.0.0</modelVersion>
  <groupId>org.cytoscape.coreplugin</groupId>
  <artifactId>cpath2</artifactId>
  <name>cpath2</name>
  <version>1.0-SNAPSHOT</version>
  <url>http://maven.apache.org</url>
  <dependencies>

    <dependency>
      <groupId>junit</groupId>
      <artifactId>junit</artifactId>
      <version>3.8.1</version>
      <scope>test</scope>
    </dependency>

    <dependency>
      <groupId>cytoscape</groupId>
      <artifactId>application</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>org.mskcc</groupId>
      <artifactId>biopax</artifactId>
      <version>1.0-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>commons-httpclient</groupId>
      <artifactId>commons-httpclient</artifactId>
      <version>3.1</version>
    </dependency>

    <dependency>
      <groupId>commons-codec</groupId>
      <artifactId>commons-codec</artifactId>
      <version>1.3</version>
    </dependency>

    <dependency>
      <groupId>commons-logging</groupId>
      <artifactId>commons-logging</artifactId>
      <version>1.1</version>
    </dependency>

  </dependencies>

        <build>
          <plugins>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>jaxb2-maven-plugin</artifactId>
                <executions>
                    <execution>
                        <id>SearchResponse</id>
                        <configuration>
                            <arguments>-nv</arguments>
                            <packageName>org.cytoscape.coreplugin.cpath2.schemas.search_response</packageName>
                            <schemaFiles>SearchResponse.xsd</schemaFiles>
                            <staleFile>${project.build.directory}/generated-sources/jaxb/org/cytoscape/coreplu
gin/cpath2/schemas/.staleFlagSearch</staleFile>
                            <clearOutputDir>false</clearOutputDir>
                        </configuration>
                        <goals>
                            <goal>xjc</goal>
                        </goals>
                    </execution>
                    <execution>
                        <id>SummaryResponse</id>
                        <configuration>
                            <arguments>-nv</arguments>
                            <packageName>org.cytoscape.coreplugin.cpath2.schemas.summary_response</packageName>
                            <schemaFiles>SummaryResponse.xsd</schemaFiles>
                            <staleFile>${project.build.directory}/generated-sources/jaxb/org/cytoscape/coreplu
gin/cpath2/schemas/.staleFlagSummary</staleFile>
                            <clearOutputDir>false</clearOutputDir>
                        </configuration>
                        <goals>
                            <goal>xjc</goal>
                        </goals>
                    </execution>
                </executions>
            </plugin>
          </plugins>
        </build>

</project>
