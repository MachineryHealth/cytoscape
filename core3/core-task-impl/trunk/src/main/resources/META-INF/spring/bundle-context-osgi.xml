<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:osgi="http://www.springframework.org/schema/osgi"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                      http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi-1.0.xsd"
	default-lazy-init="false">


	<osgi:reference id="cyNetworkNamingServiceRef"
		interface="cytoscape.util.CyNetworkNaming" />

	<osgi:reference id="cyNetworkViewFactoryServiceRef"
		interface="org.cytoscape.view.model.CyNetworkViewFactory" />

	<osgi:reference id="cyReaderManagerServiceRef"
		interface="org.cytoscape.io.read.CyReaderManager" />

	<osgi:reference id="streamUtil"
		interface="org.cytoscape.io.util.StreamUtil" />
	<!--
		<osgi:reference id="cyWriterManagerServiceRef"
		interface="org.cytoscape.io.read.CyWriterManager"/> <osgi:reference
		id="cySessionManagerServiceRef"
		interface="org.cytoscape.session.CySessionManager"/>
	-->
	<osgi:reference id="cyLayoutsServiceRef" interface="org.cytoscape.layout.CyLayouts" />

	<osgi:reference id="cyNetworkManagerServiceRef"
		interface="cytoscape.CyNetworkManager" />

	<osgi:reference id="cyPropertyServiceRef" interface="org.cytoscape.property.CyProperty"
		filter="(cyPropertyName=coreSettings)" />

	<osgi:service id="loadNetworkFileTaskFactoryService" ref="loadNetworkFileTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Load Network File..." />
			<entry key="preferredMenu" value="File.Import" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="loadNetworkURLTaskFactoryService" ref="loadNetworkURLTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Load Network URL..." />
			<entry key="preferredMenu" value="File.Import" />
		</osgi:service-properties>
	</osgi:service>
	
	<osgi:service id="loadInputStreamTaskFactoryService" ref="loadInputStreamTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Load Network InputStream..." />
			<entry key="preferredMenu" value="File.Import" />
		</osgi:service-properties>
	</osgi:service>

	<!-- selection -->
	<osgi:service id="selectAllTaskFactoryService" ref="selectAllTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Select all nodes and edges" />
			<entry key="preferredMenu" value="Select" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.CTRL_MASK | ActionEvent.ALT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="selectAllEdgesTaskFactoryService" ref="selectAllEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Select all edges" />
			<entry key="preferredMenu" value="Select.Edges" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.ALT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="selectAllNodesTaskFactoryService" ref="selectAllNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Select all nodes" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.CTRL_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>


	<osgi:service id="selectAdjacentEdgesTaskFactoryService" ref="selectAdjacentEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Select adjacent edges" />
			<entry key="preferredMenu" value="Select.Edges" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_E, ActionEvent.ALT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="selectConnectedNodesTaskFactoryService" ref="selectConnectedNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Nodes connected by selected edges" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			KeyEvent.VK_7, ActionEvent.CTRL_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="selectFirstNeighborsTaskFactoryService" ref="selectFirstNeighborsTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="First neighbors of selected nodes" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			KeyEvent.VK_6, ActionEvent.CTRL_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>


	<osgi:service id="deselectAllTaskFactoryService" ref="deselectAllTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Deselect all nodes and edges" />
			<entry key="preferredMenu" value="Select" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.CTRL_MASK | ActionEvent.SHIFT_MASK | ActionEvent.ALT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="deselectAllEdgesTaskFactoryService" ref="deselectAllEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Deselect all edges" />
			<entry key="preferredMenu" value="Select.Edges" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.ALT_MASK | ActionEvent.SHIFT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="deselectAllNodesTaskFactoryService" ref="deselectAllNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Deselect all nodes" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_A, ActionEvent.CTRL_MASK | ActionEvent.SHIFT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="invertSelectedEdgesTaskFactoryService" ref="invertSelectedEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Invert edge selection" />
			<entry key="preferredMenu" value="Select.Edges" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_I, ActionEvent.ALT_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="invertSelectedNodesTaskFactoryService" ref="invertSelectedNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Invert node selection" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_I, ActionEvent.CTRL_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="selectFromFileListTaskFactoryService" ref="selectFromFileListTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="From file..." />
			<entry key="preferredMenu" value="Select.Nodes" />
			<!--
			<entry key="acceleratorCombo" 
			java.awt.event.KeyEvent.VK_I, ActionEvent.CTRL_MASK
			-->
			<entry key="enable" value="forNetwork" />
		</osgi:service-properties>
	</osgi:service>



	<!-- show/hide -->
	<osgi:service id="hideSelectedTaskFactoryService" ref="hideSelectedTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Hide selected nodes and edges" />
			<entry key="preferredMenu" value="Select" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="hideSelectedNodesTaskFactoryService" ref="hideSelectedNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Hide selected nodes" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="hideSelectedEdgesTaskFactoryService" ref="hideSelectedEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Hide selected edges" />
			<entry key="preferredMenu" value="Select.Edges" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="unHideAllTaskFactoryService" ref="unHideAllTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Show all nodes and edges" />
			<entry key="preferredMenu" value="Select" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="unHideAllNodesTaskFactoryService" ref="unHideAllNodesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Show all nodes" />
			<entry key="preferredMenu" value="Select.Nodes" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	<osgi:service id="unHideAllEdgesTaskFactoryService" ref="unHideAllEdgesTaskFactory"
		interface="org.cytoscape.work.TaskFactory">
		<osgi:service-properties>
			<entry key="title" value="Show all edges" />
			<entry key="preferredMenu" value="Select.Edges" />
			<entry key="enable" value="forNetworkAndView" />
		</osgi:service-properties>
	</osgi:service>

	
