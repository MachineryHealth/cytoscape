<?xml version="1.0" encoding="utf-8"?>
<jnlp
  codebase="http://cytoscape.org/tut/yeastSmall/"
  href="http://cytoscape.org/tut/yeastSmall/yeastSmall.jnlp">
  <information>
    <title>Cytoscape Tutorial #2</title>
    <vendor>Cytoscape.org</vendor>
    <homepage href="http://cytoscape.org"/>
    <description>Cytoscape Tutorial #2</description> 
    <description kind="short">Visualize expression and gene annotation data on a small budding yeast network.</description>
    <offline-allowed/>
  </information>
  <security>
    <all-permissions/>
  </security>
  <resources>
    <j2se version="1.4+" max-heap-size="1024M"/>
    <jar href="cytoscape2_0_Jul182004.jar"/>
    <jar href="data.jar"/>
    <jar href="yeast-context.jar"/>
    <jar href="mcode.jar"/>
  </resources>
  <application-desc>
    <argument>-p</argument>
    <argument>jar://projectFile</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/yLayouts.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/layout.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/jgraphLayout.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/HierarchicalLayout.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/control.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/diff.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/filter.jar</argument>
    <argument>--JLW</argument>
    <argument>http://cytoscape.org/tut/common/SigAttributes.jar</argument>
    <argument>-s</argument>
    <argument>Saccharomyces cerevisiae</argument>
    <argument>-b</argument>
    <argument>jar://annotList</argument>
  </application-desc>
</jnlp>