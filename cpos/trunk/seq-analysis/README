# Partition Harbison binding regulons into chromosomal and subtelomeric groups
./partition-orflist.pl ../Harbison-data/Harbison2004_9.2_forpaper_YPD.1e-3.ORFlists ../SC-ChromosomeLocation-mtd\=30000.na

# Partition paralog groups into chromosomal and subtelomeric groups
./partition-orflist.pl AbsID.gt.60.cc.named.ORFlist ../SC-ChromosomeLocation-mtd\=30000.na 

# Commands for testing code on HXT gene sets
# more AbsID.gt.60.cc.named_SC-ChromosomeLocation-mtd\=30000.ORFlist  | grep HXT > HXT.orflist
# ln -s ~/data/fungal-sequence/sc/orf_coding.fasta .
# 
# Create fasta files of coding sequence of HXT chromosomal and subtelomeric genes
./partition-fasta.pl -o paralog paralog/HXT.orflist orf_coding.fasta 

# Run Ka/Ks ratio test
cd paralog
kaks.sh 9.GAL.HXT_chromosomal.fasta

./kaks-all.pl -o paralog paralog/AbsID.gt.60.cc.named_SC-ChromosomeLocation-mtd\=30000.ORFlist paralog/AbsID.gt.60.cc.named_SC-ChromosomeLocation-mtd\=30000.tuple 

bash run.sh
(cd paralog; R CMD BATCH kaks.R)
