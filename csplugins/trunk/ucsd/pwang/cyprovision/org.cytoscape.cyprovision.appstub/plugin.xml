<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.cytoscape.cyprovision.appstub.Application">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.cytoscape.cyprovision.appstub.application"
            name="Cytoscape">
         <property
               name="appName"
               value="Cytoscape">
         </property>
      </product>
   </extension>

</plugin>
